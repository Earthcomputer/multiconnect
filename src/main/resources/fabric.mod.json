{
  "schemaVersion": 1,
  "id": "multiconnect",
  "version": "${version}",

  "name": "MultiConnect",
  "description": "This mod allows you to connect to servers on multiple Minecraft versions",
  "authors": [
    "Earthcomputer",
    "Icon made by Vectors Market from Flaticon"
  ],
  "contact": {
    "homepage": "https://earthcomputer.net/",
    "sources": "https://github.com/Earthcomputer/multiconnect",
    "issues": "https://github.com/Earthcomputer/multiconnect/issues"
  },

  "license": "MIT",
  "icon": "assets/multiconnect/icon.png",

  "environment": "client",
  "mixins": [
    "multiconnect.mixins.json",
    "multiconnect.1_18_2.mixins.json",
    "multiconnect.1_17_1.mixins.json",
    "multiconnect.1_16_5.mixins.json",
    "multiconnect.1_16_1.mixins.json",
    "multiconnect.1_15_2.mixins.json",
    "multiconnect.1_14_4.mixins.json",
    "multiconnect.1_14_1.mixins.json",
    "multiconnect.1_14.mixins.json",
    "multiconnect.1_13_2.mixins.json",
    "multiconnect.1_12_2.mixins.json",
    "multiconnect.1_12_1.mixins.json",
    "multiconnect.1_12.mixins.json",
    "multiconnect.1_11_2.mixins.json",
    "multiconnect.1_11.mixins.json",
    "multiconnect.1_10.mixins.json",
    "multiconnect.1_9_4.mixins.json",
    "multiconnect.1_8.mixins.json"
  ],
  "accessWidener": "multiconnect.accesswidener",

  "entrypoints": {
    "modmenu": [ "net.earthcomputer.multiconnect.impl.ModMenuImpl" ]
  },

  "depends": {
    "minecraft": "${mcversion}",
    "fabricloader": ">=0.9.1"
  },

  "conflicts": {
    "immersive_portals": "*",
    "optifabric": "*"
  },

  "breaks": {
    "fabric": "<0.45.1"
  },

  "custom": {
    "lithium:options": {
      "mixin.entity.data_tracker": false
    }
  }
}
